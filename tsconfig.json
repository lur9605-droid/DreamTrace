{ 
   "compilerOptions": { 
     "target": "ESNext", 
     "lib": ["DOM", "DOM.Iterable", "ESNext"], 
     "allowJs": false, 
     "skipLibCheck": true, 
     "strict": true, 
     "noEmit": true, 
     "jsx": "preserve", 
     "incremental": true, 
     "module": "ESNext", 
     "moduleResolution": "Node", 
     "resolveJsonModule": true, 
     "isolatedModules": true, 
     "esModuleInterop": true, 
     "forceConsistentCasingInFileNames": true, 
     "paths": { 
       "@/*": ["./*"] 
     } 
   }, 
   "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "**/*.cjs"], 
   "exclude": ["node_modules"] 
 }